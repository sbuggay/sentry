{
  "name": "sentry-client",
  "private": true,
  "version": "0.0.0",
  "description": "A dashboard for collecting and displaying data from sentry-server-data.",
  "scripts": {
    "build": "webpack",
    "serve": "webpack-dev-server --content-base dist/",
    "lint": "tslint '{src,test}/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "analyze": "webpack --json | webpack-bundle-size-analyzer",
    "doc": "jsdoc"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "setupFiles": [
      "<rootDir>/test/mocks/localStorage.js"
    ]
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^2.7.8",
    "@types/jest": "^19.2.4",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.38",
    "@types/react-test-renderer": "^15.4.3",
    "@types/redux-actions": "^1.2.3",
    "@types/redux-form": "^6.6.0",
    "@types/redux-mock-store": "0.0.7",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.21.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "enzyme": "^2.8.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-test-renderer": "^15.4.2",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-mock-store": "^1.2.2",
    "redux-thunk": "^2.2.0",
    "ts-jest": "^19.0.8",
    "ts-loader": "^2.0.3",
    "tslint": "^5.1.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
