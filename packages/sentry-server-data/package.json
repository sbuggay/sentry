{
  "name": "sentry-server-data",
  "version": "0.3.5",
  "description": "A server that provides system and service information.",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bin": "./scripts/server",
  "scripts": {
    "start": "node ./scripts/server",
    "serve": "nodemon ./scripts/server --watch src",
    "build": "",
    "test": "jest . --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "author": "Devan Buggay",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/getos": "^3.0.0",
    "@types/inquirer": "0.0.43",
    "@types/jest": "^23.3.7",
    "@types/nconf": "0.0.37",
    "@types/node": "^10.12.0",
    "commander": "^2.19.0",
    "express": "^4.16.4",
    "getos": "^3.1.0",
    "inquirer": "^6.2.0",
    "json-update": "^3.0.0",
    "nconf": "^0.10.0",
    "prompt": "^1.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4"
  }
}
