{
  "name": "sentry-server-data",
  "version": "0.3.1",
  "description": "A server that provides system and service information.",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "./node_modules/.bin/ts-node src/index.ts",
    "test": "jest --coverage"
  },
  "bin": {
    "sentry-server": "./scripts/sentry-server"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "author": "Devan Buggay",
  "license": "ISC",
  "dependencies": {
    "commander": "^2.9.0",
    "express": "^4.15.2",
    "http": "0.0.0",
    "ts-node": "^3.2.0",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/jest": "^20.0.4",
    "@types/node": "^8.0.14",
    "ts-jest": "^20.0.7",
    "typescript": "^2.4.1"
  }
}
