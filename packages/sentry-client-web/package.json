{
  "name": "sentry-client-web",
  "version": "0.2.2",
  "description": "A dashboard for collecting and displaying data from sentry-server-data.",
  "scripts": {
    "build": "webpack -p",
    "serve": "webpack-dev-server --content-base dist/",
    "lint": "tslint '{src,test}/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "node ./node_modules/jest/bin/jest.js . --coverage",
    "analyze": "webpack --json | webpack-bundle-size-analyzer",
    "doc": "jsdoc"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "setupFiles": [
      "<rootDir>/test/mocks/localStorage.js"
    ]
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.7",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/redux-actions": "^2.3.1",
    "@types/redux-form": "^7.4.11",
    "@types/redux-mock-store": "^1.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "enzyme": "^3.7.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "react": "^16.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-test-renderer": "^16.6.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
